<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <context id="MySql" defaultModelType="flat">
        <plugin type="org.mybatis.generator.plugins.SerializablePlugin" />
        <jdbcConnection
                driverClass="com.mysql.jdbc.Driver"
                connectionURL="jdbc:mysql://localhost:3306/test_db"
                userId="root"
                password="666666" />

        <javaModelGenerator targetPackage="com.yangnk.redisDemo.entity" targetProject="src/main/java"></javaModelGenerator>

        <sqlMapGenerator targetPackage="mybatis.mapper"  targetProject="src/main/resources"></sqlMapGenerator>

        <javaClientGenerator targetPackage="com.yangnk.redisDemo.dao" targetProject="src/main/java" type="XMLMAPPER"></javaClientGenerator>

        <table tableName="network">
            <property name="modelOnly" value="false"/>
        </table>

    </context>

</generatorConfiguration>

